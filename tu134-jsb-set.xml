<?xml version="1.0"?>

<PropertyList>
  <sim include="Systems/tu134-jsb-views.xml">
    <description>Tu-134A</description>
    <author></author>
    <status>0.01</status>

    <flight-model>jsb</flight-model>
    <aero>tu134-jsb</aero>

    <startup>
      <splash-texture>Aircraft/Tu-134-JSB/tu134-jsb-splash.png</splash-texture>
    </startup>

    <sound>
      <path>Aircraft/Tu-134-JSB/Sounds/tu134-jsb-sound.xml</path>
    </sound>

    <model>
      <path>Aircraft/Tu-134-JSB/Models/tu134-jsb.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
      <pushback>
        <kp type="double">100</kp>
        <ki type="double">25</ki>
        <kd type="double">0</kd>
        <position-norm type="double">0</position-norm>
      </pushback>
    </model>

    <flaps>
      <setting>0.0</setting> <!--  0 deg -->
      <setting>0.2</setting> <!-- 10 deg -->
      <setting>0.4</setting> <!-- 15 deg -->
      <setting>0.6</setting> <!-- 20 deg -->
      <setting>0.8</setting> <!-- 25 deg -->
      <setting>1.0</setting> <!-- 30 deg -->
    </flaps>
    
    <systems>
      <path>Aircraft/Tu-134-JSB/Systems/tu134-jsb-systems.xml</path>
    </systems>
    
    <instrumentation>
      <path>Aircraft/Tu-134-JSB/Systems/instrumentation.xml</path>
    </instrumentation>

    <help include="Systems/help.xml"/>
    <menubar include="Dialogs/tu134-jsb-menu.xml"/>
  </sim>

  <input>
    <keyboard include="Systems/tu134-jsb-keyboard.xml"/>
  </input>

  <controls>
   <gear>
    <brake-parking type="bool">1</brake-parking>
   </gear>
  </controls>

 <engines>
  <engine n="0">
   <running>true</running>
   <reverser-position type="double">0.000000</reverser-position>
  </engine>
  <engine n="1">
   <running>true</running>
   <reverser-position type="double">0.000000</reverser-position>
  </engine>
 </engines>

  <consumables>
    <fuel>
      <tank n="0"><name>Left wing outer tank</name></tank>
      <tank n="1"><name>Left wing middle tank</name></tank>
      <tank n="2"><name>Left wing inner tank</name></tank>
      <tank n="3"><name>Central tank</name></tank>
      <tank n="4"><name>Right wing inner tank</name></tank>
      <tank n="5"><name>Right wing middle tank</name></tank>
      <tank n="6"><name>Right wing outer tank</name></tank>
    </fuel>
  </consumables>

  <payload>
    <weight>
      <name type="string">PAX</name>
      <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]"/>
      <min-lb type="double">0</min-lb>
      <max-lb type="double">18000</max-lb>
    </weight>
  </payload>

  <nasal>
    <doors>
      <file>Aircraft/Tu-134-JSB/Nasal/doors.nas</file>
    </doors>
    <tu134-jsb>
      <file>Aircraft/Tu-134-JSB/Nasal/light.nas</file>
      <file>Aircraft/Tu-134-JSB/Nasal/liveries.nas</file>
      <file>Aircraft/Tu-134-JSB/Nasal/altimeter.nas</file>
<!--  <file>Aircraft/Tu-134-JSB/Nasal/immat.nas</file> -->
    </tu134-jsb>
    <instruments>
      <file>Aircraft/Tu-134-JSB/Nasal/idr.nas</file>
    </instruments>
  </nasal>

</PropertyList>

